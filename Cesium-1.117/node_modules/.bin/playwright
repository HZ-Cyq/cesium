#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/D:/cyq/project/javascript/cesium/Cesium-1.117/node_modules/.store/@playwright+test@1.44.1/node_modules/@playwright"
else
  export NODE_PATH="$NODE_PATH:$basedir/D:/cyq/project/javascript/cesium/Cesium-1.117/node_modules/.store/@playwright+test@1.44.1/node_modules/@playwright"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.store/@playwright+test@1.44.1/node_modules/@playwright/test/cli.js" "$@"
else
  exec node  "$basedir/../.store/@playwright+test@1.44.1/node_modules/@playwright/test/cli.js" "$@"
fi
